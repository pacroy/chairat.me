<RoutingRules>
    <!-- Short URLs -->
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>abapci</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <HostName>blogs.sap.com</HostName>
            <Protocol>https</Protocol>
            <ReplaceKeyPrefixWith>2017/11/11/continuous-integration-in-abap-using-jenkins/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>aws</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <HostName>778097775924.signin.aws.amazon.com</HostName>
            <Protocol>https</Protocol>
            <ReplaceKeyPrefixWith>console/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>abaprestfulapi</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <HostName>medium.com</HostName>
            <Protocol>https</Protocol>
            <ReplaceKeyPrefixWith>pacroy/developing-apis-in-abap-just-rest-not-odata-d91cf899f7d3/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    
    <!-- Default Rules -->
    <RoutingRule>
        <Redirect>
            <HostName>medium.com</HostName>
            <Protocol>https</Protocol>
            <ReplaceKeyPrefixWith>pacroy/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
</RoutingRules>
